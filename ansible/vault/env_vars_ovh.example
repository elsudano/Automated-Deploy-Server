#!/bin/bash

# Config for DigitalOcean provider
export DIGITALOCEAN_TOKEN=""

# Config AWS provider on Terraform user
export AWS_ACCESS_KEY_ID=""
export AWS_SECRET_ACCESS_KEY=""
export AWS_DEFAULT_REGION=""

# Config ARM credentials for terraform (Azure)
export ARM_CLIENT_ID=""
export ARM_ENVIRONMENT=""
export ARM_SUBSCRIPTION_ID=""
export ARM_TENANT_ID=""

# File of credentials for google provider of terraform
export GOOGLE_CREDENTIALS=""
export CLOUDSDK_CORE_PROJECT=""
export CLOUDSDK_COMPUTE_REGION=""
export CLOUDSDK_COMPUTE_ZONE=""

# Config for OVH Provider
export OVH_ENDPOINT=
export OVH_APPLICATION_KEY=
export OVH_APPLICATION_SECRET=
export OVH_CONSUMER_KEY=

# Custom provider VmWare Workstation
export VMWS_USER=""
export VMWS_PASSWORD=""
export VMWS_URL=""
export VMWS_HTTPS=false
export VMWS_DEBUG=true

# Terraform log settings
export TF_LOG=DEBUG # INFO, DEBUG, TRACE
export TF_LOG_PATH="terraform.txt"